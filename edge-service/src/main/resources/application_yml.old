server:
  port: 8080
management:
  security:
    enabled: false
zuul:
  routes:
    order:
      path: /orders/**
      service-id: order
      strip-prefix: false
    catalog:
      path: /items/**
      service-id: catalog
      strip-prefix: false 
  ignored-services: notification 
spring:
  application:
    name: edge
eureka:
  client:
    service-url:
      defaultZone: ${EUREKA_URL:http://localhost:8761/eureka} 
security:
  user:
    name: admin
    password: password
    role:
    - SUPERUSER