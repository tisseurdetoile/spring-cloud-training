{
    "configuration": {
        "status": "error",
        "name": "JsonLogConfiguration",
        "packages": "org.apache.logging.log4j.test",
        "ThresholdFilter": {
            "level": "debug"
        },
        "appenders": {
            "Console": {
                "name": "STDOUT",
                "JSONLayout": {
                    "eventEol": true,
                    "compact": true,
                    "properties": true
                }
            }
        },
        "loggers": {
            "root": {
                "level": "info",
                "AppenderRef": {
                    "ref": "STDOUT"
                }
            }
        }
    }
}